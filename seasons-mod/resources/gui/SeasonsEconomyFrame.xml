<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<GUI name="seasonsEconomy">
    <GuiElement type="empty" profile="uiInGameMenuFrame">
        <!-- Static page header -->
        <GuiElement type="empty" profile="ingameMenuFrameHeaderPanel" position="130px -60px">
            <GuiElement type="bitmap" profile="seasonsMenuEconomyIcon" position="0px 0px"/>
            <GuiElement type="text" profile="ingameMenuFrameHeaderText" position="80px 0px" text="$l10n_seasons_ui_economy"/>
